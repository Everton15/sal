						<Param name="PortDeviceFile" value="/dev/ttyS0" />
					</parameters>
				</EndPoint>
				<Protocol name="PL40" type="PL40" />
			</LogicalPort>
			<LogicalPort name="osData">
				<EndPoint name="filesystem" type="fs" />
				<Protocol name="osdata" type="PlatformData" />
			</LogicalPort>
		</logicalPorts>
